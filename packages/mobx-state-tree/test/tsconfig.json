{
    "compilerOptions": {
        "rootDir": "../",
        "module": "commonjs",
        "lib": ["es6"],
        "target": "es6",
        "noImplicitAny": false,
        "noImplicitThis": true,
        "experimentalDecorators": true,
        "strictNullChecks": true,
        "sourceMap": false,
        "noEmit": true
